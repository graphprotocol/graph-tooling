  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^3.12.7",
    "@types/node-fetch": "^2.6.2",
    "@types/semver": "^7.3.13",
    "@types/which": "^2.0.1",
    "copyfiles": "^2.4.1",
    "jest": "26.0.0",
    "spawn-command": "0.0.2-1",
    "strip-ansi": "6.0.0",
    "tern": "0.24.3",
    "typescript": "^4.9.4"
